<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/mainFragment">


    <fragment
        android:id="@+id/mainFragment"
        android:name="ru.itis.summer24.commandproject.ui.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment2_to_landmarksFragment"
            app:destination="@id/landmarksFragment" />
        <action
            android:id="@+id/action_mainFragment_to_boardFragment"
            app:destination="@id/boardFragment" />
    </fragment>
    <fragment
        android:id="@+id/landmarksFragment"
        android:name="ru.itis.summer24.commandproject.ui.LandmarksFragment"
        android:label="fragment_landmarks"
        tools:layout="@layout/fragment_landmarks" >
        <action
            android:id="@+id/action_landmarksFragment_to_historyFragment"
            app:destination="@id/historyFragment" />
        <action
            android:id="@+id/action_landmarksFragment_to_landmarkFragment"
            app:destination="@id/landmarkFragment" />
        <action
            android:id="@+id/action_landmarksFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="ru.itis.summer24.commandproject.ui.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_landmarksFragment"
            app:destination="@id/landmarksFragment" />
        <action
            android:id="@+id/action_searchFragment_to_landmarkFragment"
            app:destination="@id/landmarkFragment" />
    </fragment>
    <fragment
        android:id="@+id/landmarkFragment"
        android:name="ru.itis.summer24.commandproject.ui.LandmarkFragment"
        android:label="fragment_landmark"
        tools:layout="@layout/fragment_landmark" >
        <action
            android:id="@+id/action_landmarkFragment_to_landmarksFragment"
            app:destination="@id/landmarksFragment" />
    </fragment>
    <fragment
        android:id="@+id/historyFragment"
        android:name="ru.itis.summer24.commandproject.ui.HistoryFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_history" >
        <action
            android:id="@+id/action_historyFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_historyFragment_to_historylandmarkFragment"
            app:destination="@id/historylandmarkFragment" />
    </fragment>
    <fragment
        android:id="@+id/historylandmarkFragment"
        android:name="ru.itis.summer24.commandproject.ui.HistoryLandmarksFragment"
        android:label="fragment_history_landmarks"
        tools:layout="@layout/fragment_history_landmarks" >
    <action
        android:id="@+id/action_historylandmarkFragment_to_historyFragment"
        app:destination="@id/historyFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="ru.itis.summer24.commandproject.ui.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/boardFragment"
        android:name="ru.itis.summer24.commandproject.BoardFragment"
        android:label="BoardFragment" >
        <action
            android:id="@+id/action_boardFragment_to_detailFragment"
            app:destination="@id/detailFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailFragment"
        android:name="ru.itis.summer24.commandproject.DetailFragment"
        android:label="fragment_detail"
        tools:layout="@layout/fragment_detail" >
        <action
            android:id="@+id/action_detailFragment_to_boardFragment"
            app:destination="@id/boardFragment" />
    </fragment>
</navigation>