<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/mainFragment">


    <fragment
        android:id="@+id/mainFragment"
        android:name="ru.itis.summer24.commandproject.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment2_to_landmarksFragment"
            app:destination="@id/landmarksFragment" />
        <action
            android:id="@+id/action_mainFragment_to_boardFragment"
            app:destination="@id/boardFragment" />
    </fragment>
    <fragment
        android:id="@+id/landmarksFragment"
        android:name="ru.itis.summer24.commandproject.LandmarksFragment"
        android:label="fragment_landmarks"
        tools:layout="@layout/fragment_landmarks" >
        <action
            android:id="@+id/action_landmarksFragment_to_dostFragment"
            app:destination="@id/dostFragment" />
        <action
            android:id="@+id/action_landmarksFragment_to_historyFragment"
            app:destination="@id/historyFragment" />
    </fragment>
    <fragment
        android:id="@+id/dostFragment"
        android:name="ru.itis.summer24.commandproject.DostFragment"
        android:label="fragment_dost"
        tools:layout="@layout/fragment_dost" />
    <fragment
        android:id="@+id/historyFragment"
        android:name="ru.itis.summer24.commandproject.HistoryFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_history" >
        <action
            android:id="@+id/action_historyFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="ru.itis.summer24.commandproject.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/boardFragment"
        android:name="ru.itis.summer24.commandproject.BoardFragment"
        android:label="BoardFragment" >
        <action
            android:id="@+id/action_boardFragment_to_detailFragment"
            app:destination="@id/detailFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailFragment"
        android:name="ru.itis.summer24.commandproject.DetailFragment"
        android:label="fragment_detail"
        tools:layout="@layout/fragment_detail" >
        <action
            android:id="@+id/action_detailFragment_to_boardFragment"
            app:destination="@id/boardFragment" />
    </fragment>
</navigation>